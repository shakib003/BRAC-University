list :\n","      self.head = Node(a[0],None)\n","      tail = self.head\n","      for i in range(1,len(a)):\n","        newNode = Node(a[i],None)\n","        tail.next = newNode\n","        tail = tail.next\n","    elif type(a) == int:\n","      if self.herad == None:\n","        self.head = Node(a,None)\n","    else:\n","      self.head = a\n","    \n","  \n","  # Traverse elements in the list.\n","  # This method is done for you. Do not change this method.\n","  def traverseList(self):\n","    s = ''\n","    temp = self.head\n","    while temp != None:\n","      if temp.next != None:\n","        s += str(temp.element) + \" \"\n","      else:\n","        s += str(temp.element)\n","      temp = temp.next\n","    return s\n","  \n","  # Count the number of nodes in the list and return the total number\n","  def countNode(self):\n","    # To Do\n","    c = 0\n","    tail = self.head\n","    while tail != None:\n","      c+=1\n","      tail = tail.next\n","    return c\n","  # returns the reference of the Node at the given index. For invalid index return None.\n","  def nodeAt(self, idx):\n","    c = 0\n","    tail = self.head\n","    while tail != None:\n","      if c != idx:\n","        c+=1\n","        tail = tail.next\n","      else:\n","        return tail\n","    return None\n","  \n","  # returns the element of the Node at the given index. For invalid idx return None.\n","  def get(self, idx):\n","    c = 0\n","    tail = self.head\n","    while tail != None:\n","      if c != idx:\n","        c+=1\n","        tail = tail.next\n","      else:\n","        return tail.element\n","    return None\n","  \n","  # updates the element of the Node at the given index. \n","  # Returns the old element that was replaced. For invalid index return None.\n","  # parameter: index, element\n","  def set(self, idx, elem):\n","    # To Do\n","    if idx >= self.countNode():\n","      return None\n","    else:\n","      c = 0\n","      tail = self.head\n","      while tail != None:\n","        if c != idx-1:\n","          c+=1\n","          tail = tail.next\n","        else:\n","          x = tail.next\n","          tail.next = Node(elem,x.next)    \n","          return x.element\n","\n","  # returns the index of the Node containing the given element.\n","  # if the element does not exist in the List, return -1.\n","  def indexOf(self, elem):\n","    c = 0\n","    tail = self.head\n","    while tail != None:\n","      if tail.element != elem:\n","        c+=1\n","        tail = tail.next\n","      else:\n","        return c\n","  \n","  # returns true if the element exists in the List, return false otherwise.\n","  def contains(self, elem):\n","    tail = self.head\n","    while tail != None:\n","      if tail.element != elem:\n","        tail = tail.next\n","        \n","      else:\n","        return True\n","    return False\n","\n","      \n","\n","  # Makes a duplicate copy of the given List. Returns the reference of the duplicate list.\n","  def copyList(self):\n","    # To Do\n","    L = LinkedList(None)\n","    L.head = self.head\n","    return L.head\n","\n","  # Makes a reversed copy of the given List. Returns the head reference of the reversed list.\n","  def reverseList(self):\n","    # To Do\n","    temp = self.head\n","    nh = None\n","    while temp != None:\n","      n = temp.next\n","      temp.next = nh\n","      nh = temp\n","      temp = n\n","    return nh\n","  \n","  # inserts Node containing the given element at the given index\n","  # Check validity of index. If invalid then print \"Invalid Index\"\n","  def insert(self, elem, idx):\n","    # To Do\n","    if idx > self.countNode():\n","      print(\"Invalid Index\")\n","    else:\n","      c = 0\n","      tail = self.head\n","\n","      while tail != None:\n","        if c == idx:\n","          n = Node(elem,None)\n","          n.next = self.head\n","          self.head = n\n","          return self.head\n","\n","        elif c != idx-1:\n","          c+=1\n","    